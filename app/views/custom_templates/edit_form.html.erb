<h1>Edit Director <%= @directore.id %></h1>

<form action="/update_director/<%= @directore.id %>">
      <div>
    <label for="name_input">
      Name
    </label>

    <input id="name_input" type="text" name="the_name" value="<%= @directore.name %>">
  </div>
  
    <div>
    <label for="bio_input">
      BIO
    </label>

    <input id="bio_input" type="text" name="the_bio" value="<%= @directore.bio %>">
  </div>
  
  <div>
    <label for="dob_input">
      DOB
    </label>

    <input id="dob_input" type="text" name="the_dob" value="<%= @directore.dob %>">
  </div>

  <div>
    <label for="image_url_input">
      Image URL
    </label>

    <input id="image_url_input" type="text" name="the_image_url" value="<%= @directore.image_url %>">
  </div>
    
    <button>
    Update Director
  </button>
</form>
